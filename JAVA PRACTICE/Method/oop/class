public class Main {
  int x = 5;

  public static void main(String[] args) {
  
    Main myObj1 = new Main();
    Main myObj2 = new Main();
    
    System.out.println(myObj1.x);
    System.out.println(myObj2.x);
    
  }
}


//set the object

public class Main {
  int x;

  public static void main(String[] args) {
    Main myObj = new Main();
    myObj.x = 40;
    System.out.println(myObj.x);
  }
}
