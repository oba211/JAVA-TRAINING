// Add Two Numbers

public class Main
{
	public static void main(String[] args) {
	    int num1=10,num2=20,ans;
	    ans=num1+num2;

		System.out.println("The sum is"+" " + ans);
	}
}


// Add two no using user Input

import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    int num1,num2,ans;
	    
	     Scanner sc = new Scanner (System.in);
	    
	     num1=sc.nextInt();
         num2=sc.nextInt();

	     ans=num1+num2;
		System.out.println("The sum is" +" " +ans);
		
	}
}
