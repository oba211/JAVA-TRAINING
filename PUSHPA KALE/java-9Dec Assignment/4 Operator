public class Main
{
	public static void main(String[] args) {
	    int num1=10,num2=20,ans;
	    ans=num1+num2;

		//System.out.println("The sum is"+" " + ans);
		
		//using not equal to operator
		System.out.println( num1!=num2);
		
		//using greater than
		System.out.println( num1>num2);
		
		//using less than
		System.out.println( num1<num2);
		
		//incremnet
		System.out.println( num2++);
		
		//pre increment
		System.out.println( ++num2);
		
		//decrement
		System.out.println( num1--);
		
		//pre decrement
		System.out.println( --num1);
	}
}
